						<xs:documentation>List of names of SU final prototypes which can be instantiated on the node</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="SUFinalPrototype" type="SUNameT" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="name" type="nodeNameT" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="nodeInstance">
		<xs:annotation>
			<xs:documentation>Cluster node Instance</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="prototypeName" type="nodeNameT" minOccurs="0">
					<xs:annotation>
						<xs:documentation>The name of the prototype which the descriptor is based on.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="nodeAttributes" type="nodeAttributesT" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Node attributes - If a node prototypeName is specified, these attributes override the ones from the prototype. </xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="SUList" minOccurs="0">
					<xs:annotation>
						<xs:documentation>List of service unit descriptors for the node. </xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="SUInstance" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>Service Unit Descriptor</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="prototypeName" type="SUNameT" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Service unit prototype name</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="SUAttributes" type="SUAttributesT" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Service Unit Attributes - If a service unit prototypeName is specified, these attributes override the ones from the prototype.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="componentList" minOccurs="0">
											<xs:annotation>
												<xs:documentation>List of component descriptors for the service unit. </xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="componentInstance" maxOccurs="unbounded">
														<xs:annotation>
															<xs:documentation>Component Descriptors</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:sequence>
																<xs:element name="prototypeName" type="componentNameT" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>Component prototype name</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="componentAttributes" type="componentAttributesT" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>Component Attributes - If a component prototypeName is specified, these attributes override the ones from the prototype.</xs:documentation>
																	</xs:annotation>
																</xs:element>
															</xs:sequence>
															<xs:attribute name="name" type="componentNameT" use="required"/>
														</xs:complexType>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
									<xs:attribute name="name" type="SUNameT" use="required"/>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="name" type="nodeNameT" use="required">
				<xs:annotation>
					<xs:documentation>The name of the node.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="nodeID" use="required">
				<xs:simpleType>
					<xs:annotation>
						<xs:documentation>The numeric identifier of the node.</xs:documentation>
					</xs:annotation>
					<xs:restriction base="xs:hexBinary">
						<xs:maxLength value="32"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<!-- ROOT ELEMENT-->
	<xs:element name="AMFConfig">
		<xs:annotation>
			<xs:documentation>Root Element for the SAF Configuration</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="componentPrototype" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="CSIPrototype" maxOccurs="unbounded"/>
				<xs:element ref="SUPrototype" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="SIInstance" maxOccurs="unbounded"/>
				<xs:element ref="SGInstance" maxOccurs="unbounded"/>
				<xs:element ref="nodePrototype" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="nodeInstance" maxOccurs="unbounded"/>
				<xs:element name="vendorExtensions" minOccurs="0">
					<xs:complexType>
						<xs:complexContent>
							<xs:extension base="vendorExtensionsT">
								<xs:sequence>
									<xs:element name="NCSScalars" minOccurs="0">
										<xs:annotation>
											<xs:documentation>NCS specific scalar mib configuration</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:sequence>
												<xs:element name="sndHbInt" type="xs:nonNegativeInteger">
													<xs:annotation>
														<xs:documentation>AVD to AVND heartbeat interval</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="rcvHbInt" type="xs:nonNegativeInteger">
													<xs:annotation>
														<xs:documentation>Avnd to AVD </xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="clusterInitTime" type="xs:nonNegativeInteger" minOccurs="0">
													<xs:annotation>
														<xs:documentation>Amount of time AVD waits during cluster initialisation before assigning SIs to SUs</xs:documentation>
													</xs:annotation>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
							</xs:extension>
						</xs:complexContent>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="cluster" type="xs:Name" use="optional"/>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="paramNameListT">
		<xs:annotation>
			<xs:documentation>General Param Name List for CSI Attributes</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ParamName" maxOccurs="unbounded">
				<xs:complexType>
					<xs:attribute name="name" type="xs:Name" use="required">
						<xs:annotation>
							<xs:documentation>The name of a generic component parameter.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="SystemConfig">
		<xs:annotation>
			<xs:documentation>Root Element for System Configuration/Description</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="NCSConfig" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Root element for the software Config</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="AISConfig" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Configuration information pertaining to SAF AIS objects</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element ref="AMFConfig" minOccurs="0" maxOccurs="unbounded"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Hardware-Deployment-Config" minOccurs="0">
					<xs:annotation>
						<xs:documentation>The hardware configuration information capturing the containment hierarchy.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="EntityDeploymentInstance"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="name" type="xs:string" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:simpleType name="AdminstateT">
		<xs:annotation>
			<xs:documentation>Data Type for the default initial admin state of an object</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="locked"/>
			<xs:enumeration value="unlocked"/>
		</xs:restriction>
	</xs:simpleType>
</xs:schema>
