## Tetware stuff ....
# Check this dir on 64 environs in future.
TET_LIBDIR=$(TET_ROOT)/lib/tet3
TET_INCDIR=$(TET_ROOT)/inc/tet3



TET_ARCHIVES= $(TET_LIBDIR)/tcm.o $(TET_LIBDIR)/libapi.a $(TET_LIBDIR)/libtcm_s.a


COMMON_SHARED_LIBRARIES=-lpthread -ldl -lm -lc -lrt -lncurses

INCLUDE= -I$(OPENSAF_INC)\
         -I$(TET_INCDIR) \
         -I$(TET_OPENSAF_HOME)/$(SERVICE_NAME)/inc \
         -I$(TET_OPENSAF_HOME)/mqsv/inc \
         -I$(TET_OPENSAF_HOME)/avsv/inc \
         -I$(TET_OPENSAF_HOME)/mbcsv/inc \
         -I$(TET_OPENSAF_HOME)/edsv/inc \
         -I$(TET_OPENSAF_HOME)/edsv/unittest/inc \
         -I$(TET_OPENSAF_HOME)/cpsv/inc \
         -I$(TET_OPENSAF_HOME)/glsv/inc \
         -I$(TET_OPENSAF_HOME)/common/inc 


