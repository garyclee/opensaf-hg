OPENSAF-SMI DEFINITIONS ::= BEGIN
    IMPORTS
    	enterprises, MODULE-IDENTITY
    FROM SNMPv2-SMI;

-- Contact Info
-- OpenSAF Foundation
-- www.opensaf.org
 
-- REVISION HISTORY
