-- Root of the OpenSAF project enterprise MIB tree
openSaf         OBJECT IDENTIFIER ::= { enterprises 32993 }

-- Subtree for common object and event definitions
openSafObjects  OBJECT IDENTIFIER ::= { openSaf 1 }

-- Sub-tree for specific object and event definitions
openSafProducts    OBJECT IDENTIFIER ::= { openSaf 2 }

-- Sub-tree for experimental definitions
openSafExp    	OBJECT IDENTIFIER ::= { openSaf 3 }

-- Sub-tree for registrations. These object identifiers may be 
-- returned as the values of SNMP queries
openSafReg    	OBJECT IDENTIFIER ::= { openSaf 4 }

-- Sub-tree for agent profiles
openSafCaps 	OBJECT IDENTIFIER ::= { openSaf 5 }

-- Subtree for experimental generic definitions
openSafGenExp      OBJECT IDENTIFIER ::= { openSafExp 1 }

-- Subtree for experimental, product specific objects and events
openSafProductsExp OBJECT IDENTIFIER ::= { openSafExp 2 }

-- Identities assigned to different modules created Under OpenSAF project
openSafModules OBJECT IDENTIFIER ::= { openSafReg 1 }
--opensafAuxModule OBJECT IDENTIFIER ::= { openSafReg 2 }

-- NetPlane Core Services
services OBJECT IDENTIFIER ::= { openSafObjects 1}

-- Availability Service
avsv 	OBJECT IDENTIFIER ::= { services 1 }

-- Interface Service
ifsv 	OBJECT IDENTIFIER ::= { services 2 }

-- Checkpoint Service
cpsv 	OBJECT IDENTIFIER ::= { services 3 }

-- Event Distribution Service
edsv 	OBJECT IDENTIFIER ::= { services 4 }

-- System Resource Monitoring
srmsv 	OBJECT IDENTIFIER ::= { services 5 }

-- Distributed Tracing Service
dtsv 	OBJECT IDENTIFIER ::= { services 6 }

-- Persistence Store and Restore service
pssv 	OBJECT IDENTIFIER ::= { services 7 }

-- Management Access Service
masv 	OBJECT IDENTIFIER ::= { services 8 }

-- NCS SNMP SubAgent
ncsSa 	OBJECT IDENTIFIER ::= { services 9 }

-- Availability Manager
avm 	OBJECT IDENTIFIER ::= { services 10 }

-- Generic for NCS 
generic OBJECT IDENTIFIER ::= { services 11 }

-- ncs Virtula Interface service
ncsVip 	OBJECT IDENTIFIER ::= { services 12 }

-- OpenSAF Fault Management service
nfm 	OBJECT IDENTIFIER ::= { services 13 }

-- Simple Software Upgrate Service
ssu  	OBJECT IDENTIFIER ::= { services 14 }

END

