ncsTestGroups		OBJECT IDENTIFIER ::= {ncsTestMibConformance 1}
ncsTestCompliances	OBJECT IDENTIFIER ::= {ncsTestMibConformance 2}


--
-- compliance statements
--
ncsTestMibCompliance  MODULE-COMPLIANCE
	STATUS	current
	DESCRIPTION
	"This section shall describe the following
	 - Define the implementation requirements specification for 
	   Agents.  
	 - A requirement specification may reference items defined in the
	   containing MIB module and/or in other MIB modules.
	 - A requirements specification names groups of object types and/or
	   events to be implemented.  The term 'implemented' for a group 
	   means that all of the items in the group are implemented. 
	 - The groups can be specified as unconditionally required, or as 
	   conditionally required, and if so, the conditions that cause
	   each group to be required must be specified in a textual description 
	   clause."
	MODULE
		MANDATORY-GROUPS {ncsTestScalarsGroup,  ncsTestTableOneGroup, 
			          ncsTestNotificationGroup}
	::= {ncsTestCompliances 1}
--
-- units of conformance
-- describe the OBJECT-GROUP and NOTIFICATION-GROUPs here. 
--
ncsTestScalarsGroup OBJECT-GROUP
      OBJECTS {ncsTestScalarsUnsigned32, ncsTestScalarsInteger32, 
	ncsTestScalarsIpAddress, ncsTestScalarsForNotify
              }
      STATUS  current
      DESCRIPTION
             "Objects required for Testing the Scalars Instrumentation."
   ::= { ncsTestGroups 1}

ncsTestTableOneGroup OBJECT-GROUP
      OBJECTS { ncsTestTableOneDisplayString,  ncsTestTableOnePhysAddress,
    	        ncsTestTableOneMacAddress,     ncsTestTableOneCounter32,
                ncsTestTableOneRowStatus,      ncsTestTableOneDiscrete
              }
      STATUS  current
      DESCRIPTION
             "Objects required for Testing the Table One."
   ::= { ncsTestGroups 2}


ncsTestTableTwoGroup OBJECT-GROUP
      OBJECTS { ncsTestTableTwoUnsigned, ncsTestTableTwoObjId 
              }
      STATUS  current
      DESCRIPTION
             "Objects required for Testing the Table Two."
   ::= { ncsTestGroups 3}


ncsTestNotificationGroup NOTIFICATION-GROUP
      NOTIFICATIONS { ncsTestNotificationScalarsObjs, ncsTestNotificationTableObjs,
		ncsTestNotificationCombo
	              }
      STATUS  current
      DESCRIPTION
             "Group of Notifications"
   ::= { ncsTestGroups 4}

END


