--This is a TEST MIB and will be used for the following. 
-- 1. SMIDUMP tool for code generation
-- 2. This MIB will be instrimented as MAB compliant application
--    to test the correctness/functionality of the OpenSAF SNMP SubAgent.

MCG-NCS-TEST-MIB DEFINITIONS ::= BEGIN 
IMPORTS 
            MODULE-IDENTITY, OBJECT-TYPE, NOTIFICATION-TYPE,  mib-2,
            Counter32, Gauge32, Integer32, IpAddress, 
            Unsigned32, TimeTicks, Opaque, Counter64
                    FROM SNMPv2-SMI 

            TEXTUAL-CONVENTION, TruthValue, StorageType, RowStatus, 
            DisplayString, PhysAddress, MacAddress, TestAndIncr, 
            AutonomousType, VariablePointer, RowPointer, TimeStamp, 
            TimeInterval, DateAndTime, StorageType, TDomain, TAddress
                    FROM SNMPv2-TC 

            ifIndex FROM RFC1213-MIB
            
            MODULE-COMPLIANCE, OBJECT-GROUP, NOTIFICATION-GROUP
                    FROM SNMPv2-CONF

	    eccgNcsModules, ncsSa
		    FROM OPENSAF-SMI;

ncsTestMibModule MODULE-IDENTITY
	LAST-UPDATED "200311281730Z"
	ORGANIZATION "OpenSAF Foundation"
	CONTACT-INFO
		"www.opensaf.org"

	DESCRIPTION
  	     "This MIB is designed to demo the OpenSAF Management Framework.

              (C) Copyright 2008 The OpenSAF Foundation

              This program is distributed in the hope that it will be useful, but
              WITHOUT ANY WARRANTY; without even the implied warranty of MERCHANTABILITY
              or FITNESS FOR A PARTICULAR PURPOSE. This file and program are licensed
              under the GNU Lesser General Public License Version 2.1, February 1999.
              The complete license can be accessed from the following location:
              http://opensource.org/licenses/lgpl-license.php
              See the Copying file included with the OpenSAF distribution for full
              licensing terms.

              Author(s): Emerson Network Power"

    ::= { eccgNcsModules 9}

ncsTestMibObjects   	OBJECT IDENTIFIER  ::= {ncsSa 10}
ncsTestMibConformance	OBJECT IDENTIFIER  ::= {ncsTestMibModule 2}

ncsTestScalars	    	OBJECT IDENTIFIER  ::= {ncsTestMibObjects 1}
ncsTestTables	    	OBJECT IDENTIFIER  ::= {ncsTestMibObjects 2}
ncsTestNotifications	OBJECT IDENTIFIER  ::= {ncsTestMibObjects 3}

--
--Describe the SCALAR objects
--

-- Need to check in testing with all its derived types like, Gauge, Gauge32, Counter32,
-- TimeTicks, TimeStamp etc. Also check with range, enum's, negative values etc..
ncsTestScalarsUnsigned32 OBJECT-TYPE
   SYNTAX        Unsigned32 
   MAX-ACCESS    read-write
   STATUS        current
   DESCRIPTION
       "An unsigned32 Scalar Object."
   ::= { ncsTestScalars 1}


-- Need to check in testing with all its derived types like, INTEGER, RowStatus,
-- TimeInterval, StorageType etc. Also check with range, enum's, negative values etc..
ncsTestScalarsInteger32 OBJECT-TYPE
   SYNTAX        Integer32
   MAX-ACCESS    read-write
   STATUS        current
   DESCRIPTION
       "An Integer32 Scalar Object, with a distinct range of values."
   ::= { ncsTestScalars 2}


-- Need to check in testing with all its derived types like, OCTETE STRING, NetworkAddress,
-- Opaque, BITS, DateAndTime, TAddress etc. Also check with range, enum's  values etc..
ncsTestScalarsIpAddress OBJECT-TYPE
   SYNTAX        IpAddress
   MAX-ACCESS    read-write
   STATUS        current
   DESCRIPTION
       "IpAddress Scalar Object."
   ::= { ncsTestScalars 3}

-- OBSOLETE object
ncsTestScalarsObsolete OBJECT-TYPE
   SYNTAX        Unsigned32
   MAX-ACCESS    read-only
   STATUS        obsolete
   DESCRIPTION
       "An unsigned32 Scalar obsolete Object."
   ::= { ncsTestScalars 4}

-- ACCESS-FOR-NOTIFY object
ncsTestScalarsForNotify OBJECT-TYPE
   SYNTAX        Opaque(SIZE(8))
   MAX-ACCESS    read-write
   STATUS        current
   DESCRIPTION
       "An Opaque Scalar Object, accessible for Notifications only."
   ::= { ncsTestScalars 5}

-- DEPRICATED object
ncsTestScalarsDepricated OBJECT-TYPE
   SYNTAX        TimeTicks
   MAX-ACCESS    read-only
   STATUS        deprecated
   DESCRIPTION
       "A TimeTicks Scalar Depricated Object"
   ::= { ncsTestScalars 6}

-- NOT-ACCESSIBLE object.
ncsTestScalarsNotAccessible OBJECT-TYPE
   SYNTAX        TimeTicks
   MAX-ACCESS    not-accessible
   STATUS        current
   DESCRIPTION
       "An unsigned32 Scalar Object, accessible for Notifications only"
   ::= { ncsTestScalars 7}

-- TRAP Object
ncsTestNotificationScalarsObjs NOTIFICATION-TYPE
   OBJECTS     { ncsTestScalarsUnsigned32, ncsTestScalarsIpAddress,
	             ncsTestScalarsInteger32 }
   STATUS      current
   DESCRIPTION
       "Notification with Scalars"
   ::= { ncsTestScalars 8 }

--
-- Test TABLE_1
--
ncsTestTableOne OBJECT-TYPE
   SYNTAX        SEQUENCE OF NcsTestTableOneEntry
   MAX-ACCESS    not-accessible
   STATUS        current
   DESCRIPTION
       "Test Table with Name 'One'. "
   ::= { ncsTestTables 1 }

ncsTestTableOneEntry OBJECT-TYPE
   SYNTAX        NcsTestTableOneEntry
   MAX-ACCESS    not-accessible
   STATUS        current
   DESCRIPTION
       "Fooo FOOO Fooo"
   INDEX       { ncsTestTableOneIpAddress }
   ::= { ncsTestTableOne 1 }

NcsTestTableOneEntry ::= SEQUENCE {
  ncsTestTableOneDisplayString		DisplayString, 
  ncsTestTableOnePhysAddress		PhysAddress,
  ncsTestTableOneMacAddress	        MacAddress,
  ncsTestTableOneTestAndIncr		TestAndIncr,
  ncsTestTableOneIpAddress	    	IpAddress,
  ncsTestTableOneGauge32Obsolete	Gauge32, 
  ncsTestTableOneCounter32	    	Counter32, 
  ncsTestTableOneDiscrete 	    	INTEGER,
  ncsTestTableOneRowStatus	    	RowStatus,
  ncsTestTableOneCounter64	    	Counter64,
  ncsTestTableOneGauge32Discrete    Gauge32,
  ncsTestTableOneOctetStringFixedLen	OCTET STRING,
  ncsTestTableOneOctetStringVarLen	OCTET STRING,
  ncsTestTableOneOctetStringDiscrete	OCTET STRING,
  ncsTestTableOneObjectIdentifier	OBJECT IDENTIFIER
}

