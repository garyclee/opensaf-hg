export PATH=$PATH:/opt/opensaf/controller/bin
if test `uname -m` = "x86_64"; then
   export LD_LIBRARY_PATH=/opt/opensaf/controller/lib64
else
   export LD_LIBRARY_PATH=/opt/opensaf/controller/lib
fi
export MIBDIRS=/usr/share/snmp/mibs
export MIBS=ALL
alias ls='ls -F'
alias ll='ls -Fl'
alias lrt='ls -lrt'
alias ..='cd ..'
alias ...='cd ..;cd ..'
ulimit -c unlimited

alias 'get_ha_state=/opt/opensaf/controller/scripts/get_ha_state'
alias 'nis_scxb=/opt/opensaf/controller/scripts/nis_scxb'