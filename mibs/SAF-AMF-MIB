
    SAF-AMF-MIB DEFINITIONS ::= BEGIN

     IMPORTS
         OBJECT-TYPE, MODULE-IDENTITY, NOTIFICATION-TYPE,
         Integer32, Counter32, Unsigned32, Counter64  
             FROM SNMPv2-SMI
         MODULE-COMPLIANCE, OBJECT-GROUP, 
         NOTIFICATION-GROUP
             FROM SNMPv2-CONF
         RowStatus, TimeInterval, TruthValue,
         TimeStamp, StorageType, DisplayString, 
         TEXTUAL-CONVENTION
             FROM SNMPv2-TC
         SnmpAdminString
                    FROM SNMP-FRAMEWORK-MIB
         SafAISErrorReturnCode
                    FROM SAF-AIS-TC-MIB
         SafTimeStamp, aisE
                    FROM SAF-TC-MIB;

     saAmfMIB MODULE-IDENTITY
         LAST-UPDATED "200507191200Z" -- 19th July' 2005
         ORGANIZATION "Service Availability Forum"
         CONTACT-INFO
             "Sayandeb Saha
              Postal: Mototorola ECC
              120 Turnpike Rd.
              Southborough, MA 01772
              USA
              E-mail: sayandeb.saha@motorola.com"

         DESCRIPTION
             "This MIB contains managed object definitions for the
              Application Management Framework AIS Service
              Version B.01, with minor adjustements for strict SMIv2
              compliance."
         ::= { aisE 2 }

     --****************************************************************

     saAmfObjects              OBJECT IDENTIFIER ::= { saAmfMIB 1 }
    
     saAmfScalars              OBJECT IDENTIFIER ::= { saAmfObjects 1 }
     saAmfTables               OBJECT IDENTIFIER ::= { saAmfObjects 2 }
     safAmfNotifications       OBJECT IDENTIFIER ::= { saAmfObjects 3 }
     saAmfCompliances          OBJECT IDENTIFIER ::= { saAmfObjects 4 }
     
     --****************************************************************
     -- AMF specified conventions
     --****************************************************************     
          
     SaAmfHAState  ::= TEXTUAL-CONVENTION 
         STATUS current
         DESCRIPTION " The different HA states defined by AMF."
         SYNTAX INTEGER { active(1),
                          standby(2),
                          quiesced(3),
                          quiescing(4) }
        
        
     SafAmfCompCapabilityModel ::= TEXTUAL-CONVENTION
         STATUS current        
         DESCRIPTION " The different component capability
                       defined by AMF. "
         SYNTAX INTEGER {  xactiveandystandby(1),
                           xactiveorystandby(2),
                           oneactiveorxstandby(3),
                           oneactiveoronestandby(4),
                           xactive(5),
                           oneactive(6),
                           nostate(7) }


     SafAmfRecommendedRecovery ::= TEXTUAL-CONVENTION
         STATUS current        
         DESCRIPTION " The component recovery procedures defined
                       by AMF."
         SYNTAX INTEGER {  none(1),
                           componentrestart(2),
                           componentfailover(3),
                           nodeswitchover(4),
                           nodefailover(5),
                           nodefailfast(6),
                           clusterreset(7) }


     --****************************************************************
     -- AMF Scalar Objects
     --****************************************************************

     saAmfAgentSpecVersion OBJECT-TYPE
         SYNTAX SnmpAdminString
         MAX-ACCESS read-only
         STATUS current
         DESCRIPTION " Version of the SAF specification this agent is
                       compliant to."

         REFERENCE " SAF-SMWG-AIS-A-00.03  "

         ::= { saAmfScalars 1 }


     saAmfAgentVendor OBJECT-TYPE
         SYNTAX SnmpAdminString
         MAX-ACCESS read-only
         STATUS current
         DESCRIPTION " Implementing vendor of this agent."
         REFERENCE " SAF-SMWG-AIS-A-00.03  "

         ::= { saAmfScalars 2 }

     saAmfAgentVendorProductRev OBJECT-TYPE
         SYNTAX Integer32
         MAX-ACCESS read-only
         STATUS current
         DESCRIPTION " Implementing vendors version number of this agent.
                       Set to 0 if undefined."
        
         REFERENCE " SAF-SMWG-AIS-A-00.03  "
        
         ::= { saAmfScalars 3 }

     saAmfServiceStartEnabled  OBJECT-TYPE
         SYNTAX TruthValue
         MAX-ACCESS read-only 
         STATUS current
         DESCRIPTION "This service is enabled to be started/stopped
                      via SNMP oprations."
         REFERENCE " SAF-SMWG-AIS-A-00.03  "
         ::= { saAmfScalars 4 }

     saAmfServiceState  OBJECT-TYPE
         SYNTAX INTEGER { running(1),
                          stopped(2),
                          startingup(3),
                          shuttingdown(4),
                          unavailable(5)
         } 

         MAX-ACCESS read-write
         STATUS current
         DESCRIPTION  " 
                 safServiceState with an SNMP get() operation 
                 determines if a service is running, stopped,
                 transitioning state, or is unavailable.
 
                 STATES :
 
                 RUNNING - service is running. SNMP set() to RUNNING
                         would start a stopped service, provided
                         safServiceStartEnabled value is TRUE.
 
                 STOPPED - service is stopped. SNMP set ()  to STOPPED would
                           stop a running service, provided
                           safServiceStartEnabled value is TRUE.
 
                 STARTING_UP - Service is transitioning to starting state.
                               Set() operations on the service at this state
                               are ignored.
 
                 SHUTTING_DOWN - Service is transitioning to stopped state.
                               Set() operations on the service at this state
                               are ignored.
 
                 UNAVAILABLE - Service is unavailable at this time. No
                               set() operations are honored.
                      
                 The support for this Object is optional.
                     "
         
         REFERENCE " SAF-SMWG-AIS-A-00.03  "
 
         ::= { saAmfScalars 5 }   

     --****************************************************************
	 -- Things NOT accounted for in this version of the MIB are:
	 -- 1. Instrumentation for the Admin operations esp. restart, adjust, swap etc. .
	 -- 2. Integration of Application Logical entity concept.
	 -- 3. Operational state of a Node.
	 -- 4. Traps related to the 'Application' as well as the 
	 --    Operational state of a node.
     --****************************************************************

     --****************************************************************
     -- AMF Specific scalar
     --****************************************************************

      saAmfClusterStartupTimeout OBJECT-TYPE
          SYNTAX      SafTimeStamp
          MAX-ACCESS  read-write 
          STATUS      current
          DESCRIPTION
              "Time in nano-seconds from cluster start
               for which the AMF waits before pre-instantiating 
               SUs and assigning SIs. A value of 0 means that the AMF
               should use a default value instead. The instant of time
               which is designated as the cluster startup time is 
               implementation dependent."
           ::= { saAmfScalars 6 }
 

     --****************************************************************
     -- AMF Node Table Objects
     --****************************************************************
          
     saAmfNodeTable OBJECT-TYPE
         SYNTAX      SEQUENCE OF SaAmfNodeTableEntry
         MAX-ACCESS  not-accessible
         STATUS      current
         DESCRIPTION
             "The Node Table identifies all the Nodes
              that are configured within a cluster and
              their configuration details"
              
         ::= { saAmfTables 1 }

     saAmfNodeTableEntry OBJECT-TYPE
         SYNTAX      SaAmfNodeTableEntry
         MAX-ACCESS  not-accessible
         STATUS      current
         DESCRIPTION
             "An entry in this table represents a
              node configuration in SAF cluster 
              environment."
         INDEX       {  saAmfNodeName  }   
         ::= { saAmfNodeTable 1 }

     SaAmfNodeTableEntry ::= SEQUENCE {
         saAmfNodeName                           SnmpAdminString,
         saAmfNodeSuFailoverProb                 SafTimeStamp,
         saAmfNodeSuFailoverMax                  Unsigned32,
         saAmfNodeAdminState                     INTEGER,
         saAmfNodeRowStatus                      RowStatus
     }

     saAmfNodeName OBJECT-TYPE 
         SYNTAX      SnmpAdminString
         MAX-ACCESS  accessible-for-notify
         STATUS      current
         DESCRIPTION
             "A name that uniquely identifies the
              node within the scope of the cluster
              for which the SAF amf configuration
              is specified. The node name represented by 
              saClmNodeName in the CLM MIB and saAmfNodeName 
              MUST be identical"                    
         ::= { saAmfNodeTableEntry 1 }

     saAmfNodeSuFailoverProb OBJECT-TYPE 
         SYNTAX      SafTimeStamp
         MAX-ACCESS  read-create
         STATUS      current
         DESCRIPTION
             "The value of the SU failover
              probation period. The value
              is in units of nano seconds.
              The minimum value is
              10 milli seconds(10000000)."
         ::= { saAmfNodeTableEntry 2 }

     
     saAmfNodeSuFailoverMax OBJECT-TYPE 
         SYNTAX      Unsigned32(1..4294967295)
         MAX-ACCESS  read-create
         STATUS      current
         DESCRIPTION
             "The maximum number of times SUs
              can failover within the probation time,
              without causing a node failover. "
         ::= { saAmfNodeTableEntry 3 }


     saAmfNodeAdminState OBJECT-TYPE 
         SYNTAX      INTEGER { locked(1), unlocked(2), shuttingdown(3) }
         MAX-ACCESS  read-create
         STATUS      current

         DESCRIPTION
             "The desired administrative status
              of the node. Enables locking, unlocking 
              or shutting down of all SUs on the node."
           DEFVAL      { unlocked }
         ::= { saAmfNodeTableEntry 4 }


     saAmfNodeRowStatus OBJECT-TYPE 
         SYNTAX      RowStatus
         MAX-ACCESS  read-create
         STATUS      current
         DESCRIPTION
             "This variable is used to create,
              modify, and/or delete a row in
              this table. saAmfNodeSuFailoverProb
              and saAmfNodeSuFailoverMax objects
              need to be set to valid values
              to make the status active. This
              can be set to delete or not in
              service only after all the
              dependent SUs and components are
              deleted"
         ::= { saAmfNodeTableEntry 5 }


     --****************************************************************
     -- AMF Service Group Table Objects
     --****************************************************************


          
     saAmfSGTable OBJECT-TYPE
         SYNTAX      SEQUENCE OF SaAmfSGTableEntry
         MAX-ACCESS  not-accessible
         STATUS      current
         DESCRIPTION
             "The Service Group (SG) Table identifies
              all SGs that are configured in the 
              cluster, each having a unique SG-name."
              
         ::= { saAmfTables 2 }

     saAmfSGTableEntry OBJECT-TYPE
         SYNTAX      SaAmfSGTableEntry
         MAX-ACCESS  not-accessible
         STATUS      current
         DESCRIPTION
             "An entry in this table represents a 
              SG in the AMF cluster."
         INDEX       {  saAmfSGName  }
         ::= { saAmfSGTable 1 }

     SaAmfSGTableEntry ::= SEQUENCE {
         saAmfSGName                             SnmpAdminString,
         saAmfSGRedModel                         INTEGER,   
         saAmfSGFailbackOption                   TruthValue,  
         saAmfSGNumPrefActiveSUs                 Unsigned32,
         saAmfSGNumPrefStandbySUs                Unsigned32,
         saAmfSGNumPrefInserviceSUs              Unsigned32,
         saAmfSGNumPrefAssignedSUs               Unsigned32,  
         saAmfSGMaxActiveSIsperSU                Unsigned32,
         saAmfSGMaxStandbySIsperSU               Unsigned32,
         saAmfSGAdminState                       INTEGER,
         saAmfSGCompRestartProb                  SafTimeStamp,
         saAmfSGCompRestartMax                   Unsigned32,
         saAmfSGSuRestartProb                    SafTimeStamp,
         saAmfSGSuRestartMax                     Unsigned32,
         saAmfSGNumCurrAssignedSU                Counter32,   
         saAmfSGNumCurrNonInstantiatedSpareSU    Counter32,  
         saAmfSGNumCurrSpareSU                   Counter32,
         saAmfSGRowStatus                        RowStatus
     }

     saAmfSGName OBJECT-TYPE 
         SYNTAX      SnmpAdminString
         MAX-ACCESS  accessible-for-notify
         STATUS      current
         DESCRIPTION
             "An octet string that uniquely identifies a
              Service Group within the scope of a cluster."
         ::= { saAmfSGTableEntry 1 }

     saAmfSGRedModel OBJECT-TYPE 
         SYNTAX      INTEGER {twoN(1),nPlusM(2),
                              nWay(3),nWayActive(4),
                              noRedundancy(5)}
         MAX-ACCESS  read-create
         STATUS      current
         DESCRIPTION
             "This value contains the SGs
              redundancy model. Could be
              either 2N, N+M, N-Way,
              N-Way Active and No Redundancy.
              Writable only when
              saAmfSGRowStatus is not active"
         ::= { saAmfSGTableEntry 2 }


     saAmfSGFailbackOption OBJECT-TYPE 
         SYNTAX      TruthValue
         MAX-ACCESS  read-create
         STATUS      current
         DESCRIPTION
             "If set to TRUE, AMF should fail back
              the SUs to the preferred configuration.
              Default value is false."
         ::= { saAmfSGTableEntry 3 }

     saAmfSGNumPrefActiveSUs OBJECT-TYPE 
         SYNTAX      Unsigned32(0..4294967295)
         MAX-ACCESS  read-create
         STATUS      current
         DESCRIPTION
             "This Parameter indicates the preferred number of active service units
              at any time. The Availability Management Framework should try to
              guarantee that this number of active service units exist for the 
              service group, if the number of in-service service units is large 
              enough. This is only applicable for nplusM redundancy model.
              Writable only when saAmfSGRowStatus is not active."
         ::= { saAmfSGTableEntry 4 }

     saAmfSGNumPrefStandbySUs OBJECT-TYPE 
         SYNTAX      Unsigned32(0..4294967295)
         MAX-ACCESS  read-create
         STATUS      current
         DESCRIPTION
             "This is only applicable for nplusM
              redundancy model. It represents the 
              preferred number of SUs that should have 
              standby role assigned in nPlusM redundancy 
              model. Writable only when saAmfSGRowStatus 
              is not active."
         ::= { saAmfSGTableEntry 5 }

     saAmfSGNumPrefInserviceSUs OBJECT-TYPE 
         SYNTAX      Unsigned32(0..4294967295)
         MAX-ACCESS  read-create
         STATUS      current
         DESCRIPTION
             "The Availability Management Framework should ensure that 
              this number of in-service service units are instantiated, 
              if possible. This parameter should be greater than or equal 
              to 2 in the case of 2N, greater than or equal to the 
              sum of saAmfSGNumPrefActiveSUs and saAmfSGNumPrefStandbySUs 
              in the case of N+M, greater than or equal to saAmfSGPrefAssignedSUs 
              in the case of N-way and N-way active redundancy models. The configuration
              of this object is optional and by default this value equals the number of 
              SUs in the SG."
         ::= { saAmfSGTableEntry 6 }

     saAmfSGNumPrefAssignedSUs OBJECT-TYPE 
         SYNTAX      Unsigned32(0..4294967295)
         MAX-ACCESS  read-create
         STATUS      current
         DESCRIPTION
             "This indicates the preferred number of assigned service units at any time. 
              The Availability Management Framework should try to guarantee
              that this number of assigned service units exist for the service group, 
              if the number of instantiated service units is large enough. This object
              is only applicable for N-way and N-WayActive redundancy models. 
              The configuration of this value is optional and the default value 
              is the preferred number of in-service SUs as designated by 
              saAmfSGNumPrefInserviceSUs."
         ::= { saAmfSGTableEntry 7 }

     saAmfSGMaxActiveSIsperSU OBJECT-TYPE 
         SYNTAX      Unsigned32(0..4294967295)
         MAX-ACCESS  read-create
         STATUS      current
         DESCRIPTION
             "The maximum number of Service Instances
              that can be assigned as active to
              a Service Unit. Not applicable for 2N & noRedundancy redmodel.
              The configuration of this object is optional and by default
              there is 'no limit' to the max number of active SIs per SU. 
              Writable only when saAmfSGRowStatus is not active."
         ::= { saAmfSGTableEntry 8 }

     saAmfSGMaxStandbySIsperSU OBJECT-TYPE 
         SYNTAX      Unsigned32(0..4294967295)
         MAX-ACCESS  read-create
         STATUS      current
         DESCRIPTION
             "The maximum number of Service Instances
              that can be assigned as standby to
              a Service Unit. Not applicable when
              saAmfSGRedModel is noRedundancy or 2N. 
              The configuration of this object is optional and by default
              there is 'no limit' to the max number of standby SIs per SU. 
              Writable only when saAmfSGRowStatus is not active."
         ::= { saAmfSGTableEntry 9 }

     saAmfSGAdminState OBJECT-TYPE 
         SYNTAX      INTEGER { locked(1), unlocked(2), shuttingdown(3) }
         MAX-ACCESS  read-create
         STATUS      current
         DESCRIPTION
             "The desired administrative status of this SG.
              Enables locking, unlocking or shutting down
              the SUs that comprise the SG"
         ::= { saAmfSGTableEntry 10 }

     saAmfSGCompRestartProb OBJECT-TYPE 
         SYNTAX      SafTimeStamp
         MAX-ACCESS  read-create
         STATUS      current
         DESCRIPTION
             "The component restart probation period.
              The value is in units of
              nano seconds. The minimum
              value is 10 milli seconds(10000000)."
         ::= { saAmfSGTableEntry 11 }


     saAmfSGCompRestartMax OBJECT-TYPE 
         SYNTAX      Unsigned32(1..4294967295)
         MAX-ACCESS  read-create
         STATUS      current
         DESCRIPTION
             "The maximum number of times a component
              can restart within the probation time,
              without causing a error escalation."
         ::= { saAmfSGTableEntry 12 }


     saAmfSGSuRestartProb OBJECT-TYPE 
         SYNTAX      SafTimeStamp
         MAX-ACCESS  read-create
         STATUS      current
         DESCRIPTION
             "The service unit restart
              probation period. The value
              is in units of
              nano seconds. The minimum
              value is 10 milli seconds(10000000)."
         ::= { saAmfSGTableEntry 13 }


     saAmfSGSuRestartMax OBJECT-TYPE 
         SYNTAX      Unsigned32(1..4294967295)
         MAX-ACCESS  read-create
         STATUS      current
         DESCRIPTION
             "The maximum number of times a
              Service unit can restart within
              the probation time, without
              causing an SU failover."
         ::= { saAmfSGTableEntry 14 }

     saAmfSGNumCurrAssignedSU OBJECT-TYPE 
         SYNTAX      Counter32
         MAX-ACCESS  read-only
         STATUS      current
         DESCRIPTION
             "Denotes the number of Service Units
              that have Service instances
              assigned to them in the Service Group."
         ::= { saAmfSGTableEntry 15 }

     saAmfSGNumCurrNonInstantiatedSpareSU OBJECT-TYPE 
         SYNTAX      Counter32
         MAX-ACCESS  read-only
         STATUS      current
         DESCRIPTION
             "Denotes the number of Service Units
              currently configured in this
              Service Group but not yet instantiated."
         ::= { saAmfSGTableEntry 16 }

     saAmfSGNumCurrSpareSU OBJECT-TYPE 
         SYNTAX      Counter32
         MAX-ACCESS  read-only
         STATUS      current
         DESCRIPTION
             "Denotes the number of Service Units
              currently instantiated in this
              Service Group but not yet assigned
              a Service instance."
         ::= { saAmfSGTableEntry 17 }

     saAmfSGRowStatus OBJECT-TYPE 
         SYNTAX      RowStatus
         MAX-ACCESS  read-create
         STATUS      current
         DESCRIPTION
             "This variable is used to create, modify, and/or delete a row in
              this table. saAmfSGRedModel,saAmfSGNumPrefActiveSUs(for nPlusM) 
              saAmfSGNumPrefStandbySUs(for nPlusM),saAmfSGCompRestartProb,
              saAmfSGSuRestartMax, saAmfSGNumPrefInserviceSUs, saAmfSGNumPrefAssignedSUs, 
              saAmfSGMaxActiveSIsperSU (for nPlusM, nWay, nWayActive),
              saAmfSGSuRestartProb, saAmfSGMaxStandbySIsperSU (for nPlusM, nWay, nWayActive),
              saAmfSGCompRestartMax objects need to be set to valid values (except if they are optional) 
              to make the status active. This can be set to delete or not-in-service only 
              after all the dependent SUs, SIs and components are deleted and the 
              saAmfSGAdminState is set to locked"
         ::= { saAmfSGTableEntry 18 }


     --****************************************************************
     -- AMF Service Unit Table Objects
     --****************************************************************
          
     saAmfSUTable OBJECT-TYPE
         SYNTAX      SEQUENCE OF SaAmfSUTableEntry
         MAX-ACCESS  not-accessible
         STATUS      current
         DESCRIPTION
             "The Service Unit (SU) Table identifies
              all SUs that are configured in the
              cluster, each having a unique SU-name."
              
         ::= { saAmfTables 3 }

     saAmfSUTableEntry OBJECT-TYPE
         SYNTAX      SaAmfSUTableEntry
         MAX-ACCESS  not-accessible
         STATUS      current
         DESCRIPTION
             "An entry in this table represents an
              SU in the AMF Cluster."
         INDEX       {  saAmfSUName  }
         ::= { saAmfSUTable 1 }

     SaAmfSUTableEntry ::= SEQUENCE {
         saAmfSUName                             SnmpAdminString,
         saAmfSURank                             Unsigned32,  
         saAmfSUNumComponents                    Unsigned32, 
         saAmfSUNumCurrActiveSIs                 Counter32,
         saAmfSUNumCurrStandbySIs                Counter32,
         saAmfSUAdminState                       INTEGER,
         saAmfSUFailOver                         TruthValue,
         saAmfSUReadinessState                   INTEGER,
         saAmfSUOperState                        INTEGER,
         saAmfSUPresenceState                    INTEGER,
         saAmfSUPreInstantiable                  TruthValue,              
         saAmfSUParentSGName                     SnmpAdminString,
         saAmfSUIsExternal                       TruthValue,
         saAmfSURowStatus                        RowStatus
     }

     saAmfSUName OBJECT-TYPE 
         SYNTAX      SnmpAdminString
         MAX-ACCESS  accessible-for-notify
         STATUS      current
         DESCRIPTION
             "An octet string that uniquely identifies a
              Service Unit within the scope of a cluster"
         ::= { saAmfSUTableEntry 1 }


     saAmfSURank OBJECT-TYPE 
         SYNTAX      Unsigned32(1..4294967295)
         MAX-ACCESS  read-create
         STATUS      current
         DESCRIPTION
             "The rank of the Service Unit in the
              Service Group that determines the
              HA state of the Service instance
              assignments. Writable only when
              saAmfSURowStatus is not active"
         ::= { saAmfSUTableEntry 2 }

     saAmfSUNumComponents OBJECT-TYPE 
         SYNTAX      Unsigned32(1..4294967295)
         MAX-ACCESS  read-create
         STATUS      current
         DESCRIPTION
             "The number of components that
              make up this SU. Writable only
              when saAmfSURowStatus is not active"
         ::= { saAmfSUTableEntry 3 }

     saAmfSUNumCurrActiveSIs OBJECT-TYPE 
         SYNTAX      Counter32
         MAX-ACCESS  read-only
         STATUS      current
         DESCRIPTION
             "The current number of Service Instances
              that are assigned as active to this
              Service Unit"
         ::= { saAmfSUTableEntry 4 }

     saAmfSUNumCurrStandbySIs OBJECT-TYPE 
         SYNTAX      Counter32
         MAX-ACCESS  read-only
         STATUS      current
         DESCRIPTION
             "The current number of Service Instances
              that are assigned as standby to this
              Service Unit"
         ::= { saAmfSUTableEntry 5 }

     saAmfSUAdminState OBJECT-TYPE 
         SYNTAX      INTEGER { locked(1), unlocked(2), shuttingdown(3) }
         MAX-ACCESS  read-create
         STATUS      current
         DESCRIPTION
             "The desired administrative status
              of this SU. Enables locking,
              unlocking or shutting down of the SU"
         ::= { saAmfSUTableEntry 6 }

     saAmfSUFailOver OBJECT-TYPE 
         SYNTAX      TruthValue
         MAX-ACCESS  read-create
         STATUS      current
         DESCRIPTION
             "When this object is set to true, the
              failover will done for the entire SU,
              even if the component recommends component
              failover. Default value is true."
         ::= { saAmfSUTableEntry 7 }

     saAmfSUReadinessState OBJECT-TYPE 
         SYNTAX      INTEGER { outOfService(1), inService(2), stopping(3) }
         MAX-ACCESS  read-only
         STATUS      current
         DESCRIPTION
             "This value Indicates the Readiness
              State of the Service Unit"
         ::= { saAmfSUTableEntry 8 }

     saAmfSUOperState OBJECT-TYPE 
         SYNTAX      INTEGER {enabled(1), disabled(2)}
         MAX-ACCESS  read-only
         STATUS      current
         DESCRIPTION
             "Indicates the operational
              State of the Service Unit."
         ::= { saAmfSUTableEntry 9 }

     saAmfSUPresenceState OBJECT-TYPE 
         SYNTAX      INTEGER {saAmfUninstantiated(1),
                              saAmfInstantiating(2),
                              saAmfInstantiated(3),
                              saAmfTerminating(4),
                              saAmfRestarting(5),
                              saAmfInstantiationFailed(6),
                              saAmfTerminationFailed(7)}
         MAX-ACCESS  read-only
         STATUS      current
         DESCRIPTION
             "Indicates the presence
              State of the Service Unit."
         ::= { saAmfSUTableEntry 10 }

     saAmfSUPreInstantiable OBJECT-TYPE 
         SYNTAX      TruthValue
         MAX-ACCESS  read-only
         STATUS      current
         DESCRIPTION
             "This object indicates if the
              SU is pre-instantiable SU or not."
         ::= { saAmfSUTableEntry 11 }

     saAmfSUParentSGName OBJECT-TYPE 
         SYNTAX      SnmpAdminString
         MAX-ACCESS  read-create
         STATUS      current
         DESCRIPTION
             "This object indicates the name of
              the parent Service Group to which 
              this SU belongs. This object needs
              to be set before the saAmfSURowStatus
              can be made active.
              Writable only when
              saAmfSURowStatus is not active"
         ::= { saAmfSUTableEntry 12 }

     saAmfSUIsExternal OBJECT-TYPE 
         SYNTAX        TruthValue
         MAX-ACCESS    read-only
         STATUS        current
         DESCRIPTION
             "This object indicates if this SU is 'external'."
         ::= { saAmfSUTableEntry 13 }

     saAmfSURowStatus OBJECT-TYPE 
         SYNTAX      RowStatus
         MAX-ACCESS  read-create
         STATUS      current
         DESCRIPTION
             "This variable is used to create,
              modify, and/or delete a row in
              this table. saAmfSURank, 
              saAmfSUNumComponents, saAmfSUParentSGName 
              objects need to be set to valid
              values to make the status active.This
              can be set to delete or not in
              service only after all the dependent
              components, as well as entries in the 
              saAmfSUsperSIRankTable containing this SU 
              are deleted and the saAmfSUAdminState 
              is locked for the SU."
         ::= { saAmfSUTableEntry 14 }



     --****************************************************************
     -- AMF Service Instance  Table Objects
     --****************************************************************

          
     saAmfSITable OBJECT-TYPE
         SYNTAX      SEQUENCE OF SaAmfSITableEntry
         MAX-ACCESS  not-accessible
         STATUS      current
         DESCRIPTION
             "The Service Instance (SI) Table identifies
              all SIs that are configured in the
              cluster, each having a
              unique SI-name."
              
         ::= { saAmfTables 4 }

     saAmfSITableEntry OBJECT-TYPE
         SYNTAX      SaAmfSITableEntry
         MAX-ACCESS  not-accessible
         STATUS      current
         DESCRIPTION
             "An entry in this table represents an
              SI in the AMF Cluster."
         INDEX       {  saAmfSIName  }
         ::= { saAmfSITable 1 }

     SaAmfSITableEntry ::= SEQUENCE {
         saAmfSIName                             SnmpAdminString,
         saAmfSIRank                             Unsigned32,
         saAmfSINumCSIs                          Unsigned32,                                                 
         saAmfSIPrefNumAssignments               Unsigned32,
         saAmfSIAdminState                       INTEGER,
         saAmfSIOperState                        INTEGER, 
         saAmfSINumCurrActiveAssignments         Counter32,
         saAmfSINumCurrStandbyAssignments        Counter32,
         saAmfSIParentSGName                     SnmpAdminString,
         saAmfSIRowStatus                        RowStatus
     }

     saAmfSIName OBJECT-TYPE 
         SYNTAX      SnmpAdminString
         MAX-ACCESS  accessible-for-notify
         STATUS      current
         DESCRIPTION
             "An octet string that
              uniquely identifies a
              Service instance."
         ::= { saAmfSITableEntry 1 }

     saAmfSIRank OBJECT-TYPE 
         SYNTAX      Unsigned32(1..4294967295)
         MAX-ACCESS  read-create
         STATUS      current
         DESCRIPTION
             "The rank of the Service Instance 
              in the Service Group that 
              determines based on the SU
              availability if this SI will be
              assigned to SUs or not."
         ::= { saAmfSITableEntry 2 }

     saAmfSINumCSIs OBJECT-TYPE 
         SYNTAX      Unsigned32(1..4294967295)
         MAX-ACCESS  read-create
         STATUS      current
         DESCRIPTION
             "The number of CSIs that
              make up this SI.
              Writable only when
              saAmfSIAdminState is locked."
         ::= { saAmfSITableEntry 3 }

     saAmfSIPrefNumAssignments OBJECT-TYPE 
         SYNTAX      Unsigned32(1..4294967295)
         MAX-ACCESS  read-create
         STATUS      current
         DESCRIPTION "This object covers 'Preferred number of active 
                      assignment per SI' in N-Way active and 'Preferred 
                      number of standby assignments per SI'in N-Way 
                      redundancy model"
         ::= { saAmfSITableEntry 4 }

     saAmfSIAdminState OBJECT-TYPE 
         SYNTAX      INTEGER { locked(1), unlocked(2), shuttingdown(3) }
         MAX-ACCESS  read-create
         STATUS      current
         DESCRIPTION
             "The desired administrative status
              of this SI. Enables locking,
              unlocking or shutting down of the SI"
         ::= { saAmfSITableEntry 5 }

     saAmfSIOperState OBJECT-TYPE 
         SYNTAX      INTEGER {enabled(1), disabled(2)}
         MAX-ACCESS  read-only
         STATUS      current
         DESCRIPTION
             "Indicates the operational
              State of the Service Instance."
         ::= { saAmfSITableEntry 6 }

     saAmfSINumCurrActiveAssignments OBJECT-TYPE 
         SYNTAX      Counter32
         MAX-ACCESS  read-only
         STATUS      current
         DESCRIPTION
             "This value denotes The current number
              of active SU assignments this SI has 
              when the redundancy model of the SG is 
              nWayActive."
         ::= { saAmfSITableEntry 7 }

     saAmfSINumCurrStandbyAssignments OBJECT-TYPE 
         SYNTAX      Counter32
         MAX-ACCESS  read-only
         STATUS      current
         DESCRIPTION
             "This value denotes The current number
              of standby SU assignments this SI has
              when the redundancy model of the SG is
              nWay."
         ::= { saAmfSITableEntry 8 }

     saAmfSIParentSGName OBJECT-TYPE 
         SYNTAX      SnmpAdminString
         MAX-ACCESS  read-create
         STATUS      current
         DESCRIPTION
             "This value denotes the name of the
              Parent SG to which this SI belongs.
              Writable only when saAmfSIRowStatus
              is not active"
         ::= { saAmfSITableEntry 9 }

     saAmfSIRowStatus OBJECT-TYPE 
         SYNTAX      RowStatus
         MAX-ACCESS  read-create
         STATUS      current
         DESCRIPTION
             "This variable is used to create,
              modify, and/or delete a row in
              this table. saAmfSIRank, 
              saAmfSIPrefNumAssignments, 
              saAmfSINumCSIs and saAmfSIParentSGName 
              objects need to be set to valid
              values to make the status active.This
              can be set to delete or not-in-service
              only after all the dependent CSIs are deleted, 
              all the saAmfSISIDepTableEntry entries 
              corresponding to this SI are deleted, all 
              entries in the saAmfSUsperSIRankTable containing 
              this SI as the index are deleted and the 
              saAmfSIAdminState is locked for the SI."
         ::= { saAmfSITableEntry 10 }



     --********************************************************************
     -- saAmfSUsperSIRankTable table enables an administrator to configure 
     -- ranked SUs per SI as required by the nWay & nWayActive Redundancy Models.  
     --********************************************************************* 


     saAmfSUsperSIRankTable OBJECT-TYPE
         SYNTAX      SEQUENCE OF SaAmfSUsperSIRankEntry
         MAX-ACCESS  not-accessible
         STATUS      current
         DESCRIPTION
             " This table enables adminstrators to configure ranked
               SUs per SI for a particular SG when the redundancy model
               of the SG is either nWay or nWayActive. There will be no entries in this
               table if the SG redundancy model is not nWay or nWayActive."
              
         ::= { saAmfTables 5 }

     saAmfSUsperSIRankEntry OBJECT-TYPE
         SYNTAX      SaAmfSUsperSIRankEntry
         MAX-ACCESS  not-accessible
         STATUS      current
         DESCRIPTION
             "An entry in this table represents an
              SI-SU ranked order value."
         INDEX       {  saAmfSUsperSIName, saAmfSUsperSIRank }
         ::= { saAmfSUsperSIRankTable 1 }


     SaAmfSUsperSIRankEntry ::= SEQUENCE {
         saAmfSUsperSIName        SnmpAdminString,
         saAmfSUsperSIRank        Unsigned32,
         saAmfSUsperSISUName      SnmpAdminString,
         saAmfSUsperSIRowStatus   RowStatus
     }

     saAmfSUsperSIName OBJECT-TYPE 
         SYNTAX        SnmpAdminString
         MAX-ACCESS    accessible-for-notify
         STATUS        current
         DESCRIPTION
             "An octet string that uniquely identifies a
              Service instance which is being configured with ranked
              SUs."
         ::= { saAmfSUsperSIRankEntry 1 }


      saAmfSUsperSIRank OBJECT-TYPE 
         SYNTAX         Unsigned32
         MAX-ACCESS     accessible-for-notify
         STATUS         current
         DESCRIPTION    "The rank of the SU for the SI 
                         designated by saAmfSUsperSIName."
         ::= { saAmfSUsperSIRankEntry 2 }


      saAmfSUsperSISUName OBJECT-TYPE 
         SYNTAX           SnmpAdminString
         MAX-ACCESS       read-create
         STATUS           current
         DESCRIPTION
             "An octet string that uniquely identifies a
              Service Unit which is being configured with rank
              designated by saAmfSUsperSIRank for the SI designated
              by saAmfSUsperSIName."
         ::= { saAmfSUsperSIRankEntry 3 }

      saAmfSUsperSIRowStatus OBJECT-TYPE 
         SYNTAX              RowStatus
         MAX-ACCESS          read-create
         STATUS              current
         DESCRIPTION
             "This variable is used to create,
              modify, and/or delete a row in
              this table. saAmfSUsperSISUName
              object needs to be set to a valid value
              to make the status active.
              This variable can be set to destroy or notinservice only if the
              adminstate of the SI designated by saAmfSUsperSIName and the SU 
              designated by saAmfSUsperSISUName columnar value is LOCKED. 
              No object in this table should be modified
              when the value of rowstatus is active."
         ::= { saAmfSUsperSIRankEntry 4 }


     --*********************************************************************
     -- AMF SG-SI-Rank table. This is a read-only table that enables
     -- an administrator to view an ordered list of SIs per SG based on rank
     --*********************************************************************
     saAmfSGSIRankTable OBJECT-TYPE
         SYNTAX      SEQUENCE OF SaAmfSGSIRankEntry
         MAX-ACCESS  not-accessible
         STATUS      current
         DESCRIPTION
             " This is a read-only table that enables an administrator
               to view the ordered list of SIs per SG based on rank."
         ::= { saAmfTables 6 }

     saAmfSGSIRankEntry OBJECT-TYPE
         SYNTAX      SaAmfSGSIRankEntry
         MAX-ACCESS  not-accessible
         STATUS      current
         DESCRIPTION
             "An entry in this table represents an
              SG-SI ranked order value."
         INDEX       {  saAmfSGSIRankSGName, saAmfSGSIRank}
         ::= { saAmfSGSIRankTable 1 }


      SaAmfSGSIRankEntry ::= SEQUENCE {
         saAmfSGSIRankSGName      SnmpAdminString,
         saAmfSGSIRank            Unsigned32,
         saAmfSGSIRankSIName      SnmpAdminString
      }

      saAmfSGSIRankSGName OBJECT-TYPE 
         SYNTAX           SnmpAdminString
         MAX-ACCESS       accessible-for-notify
         STATUS            current
         DESCRIPTION
             "An octet string that uniquely identifies a
              Service Group which is being configured with ranked
              SIs."
         ::= { saAmfSGSIRankEntry 1 }


      saAmfSGSIRank     OBJECT-TYPE 
         SYNTAX         Unsigned32
         MAX-ACCESS     accessible-for-notify
         STATUS         current
         DESCRIPTION    "The rank of the SI for the SG 
                         designated by saAmfSGSIRankSGName."
         ::= { saAmfSGSIRankEntry 2 }


      saAmfSGSIRankSIName OBJECT-TYPE 
         SYNTAX           SnmpAdminString
         MAX-ACCESS       read-create
         STATUS           current
         DESCRIPTION
             "An octet string that uniquely identifies a
              Service Instance which is being configured with rank
              designated by saAmfSGSIRank for the SG designated
              by saAmfSGSIRankSGName."
         ::= { saAmfSGSIRankEntry 3 }




     --*********************************************************************
