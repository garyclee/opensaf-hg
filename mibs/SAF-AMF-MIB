     --****************************************************************
     -- AMF Service Instance dependencies Table Objects
     --****************************************************************

          
     saAmfSISIDepTable OBJECT-TYPE
         SYNTAX      SEQUENCE OF SaAmfSISIDepTableEntry
         MAX-ACCESS  not-accessible
         STATUS      current
         DESCRIPTION
             "The Service Instance-Service Instance (SI-SI)
              dependency Table identifies the dependencies
              between SIs that are configured in the
              saAmfSITable."
              
         ::= { saAmfTables 8 }

     saAmfSISIDepTableEntry OBJECT-TYPE
         SYNTAX      SaAmfSISIDepTableEntry
         MAX-ACCESS  not-accessible
         STATUS      current
         DESCRIPTION
             "An entry in this table represents an
              SI-SI dependency in the AMF Cluster."
         INDEX       {  saAmfSISIDepSIName , saAmfSISIDepDepndSIName }
         ::= { saAmfSISIDepTable 1 }

     SaAmfSISIDepTableEntry ::= SEQUENCE {
         saAmfSISIDepSIName                      SnmpAdminString,
         saAmfSISIDepDepndSIName                 SnmpAdminString,
         saAmfSISIDepToltime                     SafTimeStamp,
         saAmfSISIDepRowStatus                   RowStatus
     }

     saAmfSISIDepSIName OBJECT-TYPE 
         SYNTAX      SnmpAdminString
         MAX-ACCESS  accessible-for-notify
         STATUS      current
         DESCRIPTION
             "An octet string that uniquely identifies a
              Service instance for which saAmfSIRowStatus
              is active."
         ::= { saAmfSISIDepTableEntry 1 }

     saAmfSISIDepDepndSIName OBJECT-TYPE 
         SYNTAX      SnmpAdminString
         MAX-ACCESS  accessible-for-notify
         STATUS      current
         DESCRIPTION
             "An octet string that uniquely identifies a
              Service instance for which saAmfSIRowStatus
              is active. This SI is dependent on the 
              saAmfSISIDepSIName to be enabled for it
              to be enabled."
         ::= { saAmfSISIDepTableEntry 2 }
         
     saAmfSISIDepToltime OBJECT-TYPE 
         SYNTAX      SafTimeStamp
         MAX-ACCESS  read-create
         STATUS      current
         DESCRIPTION
             "The amount of time AMF waits in nano-seconds 
              to disable saAmfSISIDepDepndSIName SI, if 
              saAmfSISIDepSIName SI becomes disabled."
         ::= { saAmfSISIDepTableEntry 3 }

     saAmfSISIDepRowStatus OBJECT-TYPE 
         SYNTAX      RowStatus
         MAX-ACCESS  read-create
         STATUS      current
         DESCRIPTION
             "This variable is used to create,
              modify, and/or delete a row in
              this table. saAmfSISIDepToltime
              needs to be set to valid
              value to make the status active."
         ::= { saAmfSISIDepTableEntry 4 }

     --****************************************************************
     -- AMF Component  Table Objects
     --****************************************************************
          
     saAmfCompTable OBJECT-TYPE
         SYNTAX      SEQUENCE OF SaAmfCompTableEntry
         MAX-ACCESS  not-accessible
         STATUS      current
         DESCRIPTION
             "The Component Table identifies
              all Components that are configured in the
              cluster, each having a
              unique component-name."
              
         ::= { saAmfTables 9 }

     saAmfCompTableEntry OBJECT-TYPE
         SYNTAX      SaAmfCompTableEntry
         MAX-ACCESS  not-accessible
         STATUS      current
         DESCRIPTION
             "An entry in this table represents a 
              component in the AMF cluster"
         INDEX       {  saAmfCompName  }
         ::= { saAmfCompTable 1 }

     SaAmfCompTableEntry ::= SEQUENCE {
         saAmfCompName                                  SnmpAdminString,
         saAmfCompCapability                            SafAmfCompCapabilityModel,
         saAmfCompCategory                              INTEGER,
         saAmfCompInstantiateCmd                        DisplayString,
         saAmfCompTerminateCmd                          DisplayString,
         saAmfCompCleanupCmd                            DisplayString,
         saAmfCompAmStartCmd                            DisplayString,
         saAmfCompAmStopCmd                             DisplayString,
         saAmfCompInstantiationLevel                    Unsigned32,
         saAmfCompInstantiateTimeout                    SafTimeStamp,
         saAmfCompDelayBetweenInstantiateAttempts       SafTimeStamp,
         saAmfCompTerminateTimeout                      SafTimeStamp,
         saAmfCompCleanupTimeout                        SafTimeStamp,
         saAmfCompAmStartTimeout                        SafTimeStamp,
         saAmfCompAmStopTimeout                         SafTimeStamp,
         saAmfCompTerminateCallbackTimeOut              SafTimeStamp,
         saAmfCompCSISetCallbackTimeout                 SafTimeStamp,
         saAmfCompQuiescingCompleteTimeout              SafTimeStamp,
         saAmfCompCSIRmvCallbackTimeout                 SafTimeStamp,
         saAmfCompProxiedCompInstantiateCallbackTimeout SafTimeStamp,
         saAmfCompProxiedCompCleanupCallbackTimeout     SafTimeStamp,
         saAmfCompNodeRebootCleanupFail                 TruthValue,
         saAmfCompRecoveryOnError                       SafAmfRecommendedRecovery,
         saAmfCompNumMaxInstantiate                     Unsigned32,
         saAmfCompNumMaxInstantiateWithDelay            Unsigned32,
         saAmfCompNumMaxAmStartAttempts                 Unsigned32,
         saAmfCompNumMaxAmStopAttempts                  Unsigned32,
         saAmfCompDisableRestart                        TruthValue,
         saAmfCompNumMaxActiveCsi                       Unsigned32,
         saAmfCompNumMaxStandbyCsi                      Unsigned32,    
         saAmfCompNumCurrActiveCsi                      Counter32,
         saAmfCompNumCurrStandbyCsi                     Counter32,
         saAmfCompRestartCount                          Counter32,
         saAmfCompOperState                             INTEGER,
         saAmfCompPresenceState                         INTEGER,
         saAmfCompReadinessState                        INTEGER,
         saAmfCompCurrProxyName                         SnmpAdminString,
         saAmfCompAMEnable                              INTEGER,
         saAmfCompRowStatus                             RowStatus
     }

     saAmfCompName OBJECT-TYPE 
         SYNTAX      SnmpAdminString
         MAX-ACCESS  accessible-for-notify
         STATUS      current
         DESCRIPTION
             "An octet string that
              uniquely identifies a component"
         ::= { saAmfCompTableEntry 1 }

     saAmfCompCapability OBJECT-TYPE 
         SYNTAX      SafAmfCompCapabilityModel
         MAX-ACCESS  read-create
         STATUS      current
         DESCRIPTION
             "This value denotes the Capability
              of the component as defined by
              SAFs Component Capability Model.
              Writable only when
              saAmfCompRowStatus is not active"
         ::= { saAmfCompTableEntry 2 }

     saAmfCompCategory OBJECT-TYPE 
         SYNTAX      INTEGER { saAware(0),
                               proxiedLocalPreinstantiable(1),
                               proxiedLocalNonPreinstantiable(2),
                               externalPreinstantiable(3),	
                               externalNonPreinstantiable(4),
                               unproxiedNonSaAware(5)  }
         MAX-ACCESS  read-create
         STATUS      current
         DESCRIPTION
             "This value denotes the component type.
              External components are always proxied, therefore the
              'proxied' word has been left out in order to restrict 
              the identifier's length. Writable only when
              saAmfCompRowStatus is not active"
         ::= { saAmfCompTableEntry 3 }

     saAmfCompInstantiateCmd OBJECT-TYPE 
         SYNTAX      DisplayString
         MAX-ACCESS  read-create
         STATUS      current
         DESCRIPTION
             "This value denotes the Instantiate
              command of the component. This command is not applicable for 
              components whose saAmfCompCategory value is either of the following...
              proxiedLocalPreinstantiable, proxiedLocalNonPreinstantiable,
              externalPreinstantiable and externalNonPreinstantiable."
         ::= { saAmfCompTableEntry 4 }

     saAmfCompTerminateCmd OBJECT-TYPE 
         SYNTAX      DisplayString
         MAX-ACCESS  read-create
          STATUS      current
         DESCRIPTION
             "This value denotes the Terminate
              script of the component. This
              command is applicable for 
              components with saAmfCompCategory value as
              unproxiedNonSaAware."
         ::= { saAmfCompTableEntry 5 }

     saAmfCompCleanupCmd OBJECT-TYPE 
         SYNTAX      DisplayString
         MAX-ACCESS  read-create
         STATUS      current
         DESCRIPTION
             "This value denotes the Cleanup
              command of the component."
         ::= { saAmfCompTableEntry 6 }

     saAmfCompAmStartCmd OBJECT-TYPE 
         SYNTAX      DisplayString
         MAX-ACCESS  read-create
         STATUS      current
         DESCRIPTION
             "This value denotes the Application Monitor 
              Start command of the component."
         ::= { saAmfCompTableEntry 7 }

     saAmfCompAmStopCmd OBJECT-TYPE 
         SYNTAX      DisplayString
         MAX-ACCESS  read-create
         STATUS      current
         DESCRIPTION
             "This value denotes the Application Monitor
              Stop command of the component."
         ::= { saAmfCompTableEntry 8 }

     saAmfCompInstantiationLevel OBJECT-TYPE 
         SYNTAX      Unsigned32(1..4294967295)
         MAX-ACCESS  read-create
         STATUS      current
         DESCRIPTION
             "This value determines the order in which
              components in an SU are instantiated and 
              terminated. This object is aplicable only
              for pre-instantiable components.
              Writable only when
              saAmfCompRowStatus is not active"
         ::= { saAmfCompTableEntry 9 }

     saAmfCompInstantiateTimeout OBJECT-TYPE 
         SYNTAX      SafTimeStamp
         MAX-ACCESS  read-create
         STATUS      current
         DESCRIPTION
             "Time in nano-seconds in which the AMF 
              expects the instantiate command to
              complete and the component registers
              with AMF by calling saAmfComponentRegister
              API. A value of 0 means that the AMF 
              should use a default value instead."
         ::= { saAmfCompTableEntry 10 }

     saAmfCompDelayBetweenInstantiateAttempts OBJECT-TYPE 
         SYNTAX      SafTimeStamp
         MAX-ACCESS  read-create
         STATUS      current
         DESCRIPTION
             "Time in nano-seconds, is the delay
              period AMF waits before attempting 
              to instantiate a component after 
              failing to instantiate it within
              saAmfCompNumMaxInstantiate attempts.
              A value of 0 means that the AMF 
              should use a default value instead."
         ::= { saAmfCompTableEntry 11 }          

     saAmfCompTerminateTimeout OBJECT-TYPE 
         SYNTAX      SafTimeStamp
         MAX-ACCESS  read-create
         STATUS      current
         DESCRIPTION
             "Time in nano-seconds in which the AMF expects 
              the terminate CLC-CLI command to return success. 
              A value of 0 means that the AMF should use a 
              default value instead. If a component has not 
              responded within saAmfCompTerminateTimeout 
              it will be shut down by executing the cleanup command"
         ::= { saAmfCompTableEntry 12 }

     saAmfCompCleanupTimeout OBJECT-TYPE 
         SYNTAX      SafTimeStamp
         MAX-ACCESS  read-create
         STATUS      current
         DESCRIPTION
             "Time in nano-seconds in which the AMF expects 
              the cleanup CLC-CLI command to return success. 
              A value of 0 means that the AMF should use a 
              default value instead. "
         ::= { saAmfCompTableEntry 13 }

     saAmfCompAmStartTimeout OBJECT-TYPE 
         SYNTAX      SafTimeStamp
         MAX-ACCESS  read-create
         STATUS      current
         DESCRIPTION
             "Time in nano-seconds in which the AMF 
              expects the AM_START command to
              complete. A value of 0 means that the AMF 
              should use a default value instead."
         ::= { saAmfCompTableEntry 14 }     

     saAmfCompAmStopTimeout OBJECT-TYPE 
         SYNTAX      SafTimeStamp
         MAX-ACCESS  read-create
         STATUS      current
         DESCRIPTION
             "Time in nano-seconds in which the AMF 
              expects the AM_STOP command to
              complete. A value of 0 means that the AMF 
              should use a default value instead."
         ::= { saAmfCompTableEntry 15 }

     saAmfCompTerminateCallbackTimeOut OBJECT-TYPE 
         SYNTAX      SafTimeStamp
         MAX-ACCESS  read-create
         STATUS      current
         DESCRIPTION
             "Time in nano-seconds in which the AMF expects an 
              SaAmfResponse call from the component after a preceding 
              callback initiation. A value of 0 means that the AMF 
              should use a default value instead."
         ::= { saAmfCompTableEntry 16 }

     saAmfCompCSISetCallbackTimeout OBJECT-TYPE 
         SYNTAX      SafTimeStamp
         MAX-ACCESS  read-create
         STATUS      current
         DESCRIPTION
             "Time in nano-seconds in which the AMF expects an 
              SaAmfResponse call from the component after a preceding 
              CSI set callback initiation. A value of 0 means that the AMF 
              should use a default value instead."
         ::= { saAmfCompTableEntry 17 }

     saAmfCompQuiescingCompleteTimeout OBJECT-TYPE 
         SYNTAX      SafTimeStamp
         MAX-ACCESS  read-create
         STATUS      current
         DESCRIPTION
             "Time in nano-seconds in which the AMF expects an 
              saAmfCSIQuiescingComplete() call from the component after a preceding 
              CSI set callback initiation from active->quiescing HA state. 
              Note this is not a timeout for saAMFResponse().
              A value of 0 means that the AMF should use a default value instead."
         ::= { saAmfCompTableEntry 18 }

     saAmfCompCSIRmvCallbackTimeout OBJECT-TYPE 
         SYNTAX      SafTimeStamp
         MAX-ACCESS  read-create
         STATUS      current
         DESCRIPTION
             "Time in nano-seconds in which the AMF expects an 
              SaAmfResponse call from the component after a preceding 
              CSI Remove callback initiation. A value of 0 means that the AMF 
              should use a default value instead."
         ::= { saAmfCompTableEntry 19 }

     saAmfCompProxiedCompInstantiateCallbackTimeout OBJECT-TYPE 
         SYNTAX      SafTimeStamp
         MAX-ACCESS  read-create
         STATUS      current
         DESCRIPTION
             "Time in nano-seconds in which the AMF expects an 
              SaAmfResponse call from the component after a preceding 
              proxied component instantiate callback initiation. 
              A value of 0 means that the AMF should use a default 
              value instead."
         ::= { saAmfCompTableEntry 20 }

     saAmfCompProxiedCompCleanupCallbackTimeout OBJECT-TYPE 
         SYNTAX      SafTimeStamp
         MAX-ACCESS  read-create
         STATUS      current
         DESCRIPTION
             "Time in nano-seconds in which the AMF expects an 
              SaAmfResponse call from the component after a preceding 
              proxied component cleanup callback initiation. 
              A value of 0 means that the AMF should use a default 
              value instead."
         ::= { saAmfCompTableEntry 21 }


     saAmfCompNodeRebootCleanupFail OBJECT-TYPE 
         SYNTAX      TruthValue
         MAX-ACCESS  read-create
         STATUS      current
         DESCRIPTION
             "If set to TRUE on CLEANUP script failure AMF will
              do a node failover and node reboot. If false the
              SIs having CSIs assigned to this component will
              be unassigned."
         ::= { saAmfCompTableEntry 22 }

     saAmfCompRecoveryOnError OBJECT-TYPE 
         SYNTAX      SafAmfRecommendedRecovery
         MAX-ACCESS  read-create
         STATUS      current
         DESCRIPTION
             "Recommended Recovery configured per component to be engaged
              in the following cases:
               a) When saAmfComponentErrorReport is received with recommended recovery 
                  SA_AMF_NO_RECOMMENDATION.
               b) Timeouts for AMF callbacks.
               c) Error return codes from the application process, 
                  when a callback was invoked.
               d) There is a communication loss between the AMF Area server 
                  and the AMF library linked into a component.
                 
              The Recommended Recovery of a component can never be set to
              SA_AMF_NO_RECOMMENDATION."
         ::= { saAmfCompTableEntry 23 }

     saAmfCompNumMaxInstantiate OBJECT-TYPE 
         SYNTAX      Unsigned32(1..4294967295)
         MAX-ACCESS  read-create
         STATUS      current
         DESCRIPTION
             "This value denotes the maximum number of
              times AMF tries to instantiate the
              component if it fails during instatiation."
         ::= { saAmfCompTableEntry 24 }

     saAmfCompNumMaxInstantiateWithDelay OBJECT-TYPE 
         SYNTAX      Unsigned32(1..4294967295)
         MAX-ACCESS  read-create
         STATUS      current
         DESCRIPTION
             "This value denotes the maximum number of
              times AMF tries to instantiate the
              component with delay, if it fails to
              instatiate after saAmfCompNumMaxInstantiate
              retries. After the specified count it will place
              the components saAmfCompPresenceState in
              saAmfInstantiationFailed. If this object is not
              configured then it will show the value as 0,
              but value 0 can not be configured." 
         ::= { saAmfCompTableEntry 25 }

     saAmfCompNumMaxAmStartAttempts OBJECT-TYPE 
         SYNTAX      Unsigned32(1..4294967295)
         MAX-ACCESS  read-create
         STATUS      current
         DESCRIPTION
             "This value denotes the maximum number of
              times AMF tries to re issue the AM_START CLI
              command if it fails. "
         ::= { saAmfCompTableEntry 26 }

     saAmfCompNumMaxAmStopAttempts OBJECT-TYPE 
         SYNTAX      Unsigned32(1..4294967295)
         MAX-ACCESS  read-create
         STATUS      current
         DESCRIPTION
             "This value denotes the maximum number of
              times AMF tries to re issue the AM_STOP CLI
              command if it fails. If this object is not
              configured then it will show the value as 0,
              but value 0 can not be configured. "
         ::= { saAmfCompTableEntry 27 }

     saAmfCompDisableRestart OBJECT-TYPE 
         SYNTAX      TruthValue
         MAX-ACCESS  read-create
         STATUS      current
         DESCRIPTION
             "If set to TRUE, disables restarting
              the component as part of recovery.
              Writable only when saAmfCompRowStatus 
              is not active"
         ::= { saAmfCompTableEntry 28 }

     saAmfCompNumMaxActiveCsi OBJECT-TYPE 
         SYNTAX      Unsigned32(1..4294967295)
         MAX-ACCESS  read-create
         STATUS      current
         DESCRIPTION
             "This value denotes the maximum number of
              CSIs that can be assigned as active to
              this component. Aplicable only if
              saAmfCompCapability is xactiveandystandby
              xactiveorystandby,xactive. Writable only 
              when saAmfCompRowStatus is not active"
         ::= { saAmfCompTableEntry 29 }

     saAmfCompNumMaxStandbyCsi OBJECT-TYPE 
         SYNTAX      Unsigned32(1..4294967295)
         MAX-ACCESS  read-create
         STATUS      current
         DESCRIPTION
             "This value denotes the maximum number of
              CSIs that can be assigned as standby to
              this component. Aplicable only if
              saAmfCompCapability is xactiveandystandby
              xactiveorystandby,oneactiveorxstandby. 
              Writable only when saAmfCompRowStatus
              is not active."
         ::= { saAmfCompTableEntry 30 }

     saAmfCompNumCurrActiveCsi OBJECT-TYPE 
         SYNTAX      Counter32
         MAX-ACCESS  read-only
         STATUS      current
         DESCRIPTION
             "This value denotes the number of
              CSI relationships that have been
              assigned as active to this component."
         ::= { saAmfCompTableEntry 31 }

     saAmfCompNumCurrStandbyCsi OBJECT-TYPE 
         SYNTAX      Counter32
         MAX-ACCESS  read-only
         STATUS      current
         DESCRIPTION
             "This value denotes the number of
              CSI relationships that have been
              assigned as standby to this component."
         ::= { saAmfCompTableEntry 32 }

     saAmfCompRestartCount OBJECT-TYPE 
         SYNTAX      Counter32
         MAX-ACCESS  read-only
         STATUS      current
         DESCRIPTION
             "Indicates the number of component
              restarts."
         ::= { saAmfCompTableEntry 33 }

     saAmfCompOperState OBJECT-TYPE 
         SYNTAX      INTEGER {enabled(1), disabled(2)}
         MAX-ACCESS  read-only
         STATUS      current
         DESCRIPTION
             "Indicates the operational
              State of the component."
         ::= { saAmfCompTableEntry 34 }


     saAmfCompPresenceState OBJECT-TYPE 
         SYNTAX      INTEGER {saAmfUninstantiated(1),
                              saAmfInstantiating(2),
                              saAmfInstantiated(3),
                              saAmfTerminating(4),
                              saAmfRestarting(5),
                              saAmfInstantiationFailed(6),
                              saAmfTerminationFailed(7)}
         MAX-ACCESS  read-only
         STATUS      current
         DESCRIPTION
             "Indicates the presence
              State of the component."
         ::= { saAmfCompTableEntry 35 }

     saAmfCompReadinessState OBJECT-TYPE 
         SYNTAX      INTEGER { outOfService(1), inService(2), stopping(3) }
         MAX-ACCESS  read-only
         STATUS      current
         DESCRIPTION
             "This value Indicates the Readiness
              State of the component."
         ::= { saAmfCompTableEntry 36 }

     saAmfCompCurrProxyName OBJECT-TYPE 
         SYNTAX      SnmpAdminString
         MAX-ACCESS  read-only
         STATUS      current
         DESCRIPTION
             "This value denotes the proxy component
              that manages this component. This
              object is only applicable if component's 
              saAmfCompCategory is proxiedLocalPreinstantiable,
              proxiedLocalNonPreinstantiable, externalPreinstantiable
              or externalNonPreinstantiable. Writable                
              only when saAmfCompRowStatus is not active."
         ::= { saAmfCompTableEntry 37 }

     saAmfCompAMEnable OBJECT-TYPE 
        SYNTAX       INTEGER { enabled(1),  disabled(2) }
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "This object is used to enable / disable active monitoring. It  
            is set only when AM_START and AM_STOP commands are configured."
        ::= { saAmfCompTableEntry 38 }

     saAmfCompRowStatus OBJECT-TYPE 
         SYNTAX      RowStatus
         MAX-ACCESS  read-create
         STATUS      current
         DESCRIPTION
             "This variable is used to create,
              modify, and/or delete a row in
              this table. 
              saAmfCompCapability
              saAmfCompCategory,saAmfCompInstantiateCmd,
              saAmfCompTerminateCmd,saAmfCompCleanupCmd,
              saAmfCompAmStartCmd,saAmfCompAmStopCmd,
              saAmfCompRecoveryOnError,
              saAmfCompNumMaxInstantiate,
              saAmfCompNumMaxActiveCsi, 
              saAmfCompNumMaxStandbyCsi 
              objects need to be set to valid
              values to make the status active.This
              can be set to delete or not in
              service only if the saAmfSUAdminState and
              saAmfSUPresenceState of the SU to which this
              component belongs are locked and 
              saAmfUninstantiated or saAmfTerminationFailed
              respectively."
         ::= { saAmfCompTableEntry 39 }

     --****************************************************************
     -- AMF Component-CompServiceType supported table. This tables 
     -- enables configuring various component services instance types 
     -- supported by a component on a per component basis.
     --****************************************************************
     saAmfCompCSTypeSupportedTable OBJECT-TYPE
         SYNTAX      SEQUENCE OF SaAmfCompCSTypeSupportedTableEntry
         MAX-ACCESS  not-accessible
         STATUS      current
         DESCRIPTION
             "The Component-CompServiceType supported table identifies
              all the CSI types each of the Components configured in the
              cluster support."
              
         ::= { saAmfTables 10 }

     saAmfCompCSTypeSupportedTableEntry OBJECT-TYPE
         SYNTAX      SaAmfCompCSTypeSupportedTableEntry
         MAX-ACCESS  not-accessible
         STATUS      current
         DESCRIPTION
             "An entry in this table represents a 
              CS Type supported by a component."
         INDEX   {  saAmfCompCSTypeSupportedCompName, saAmfCompCSTypeSupportedCSIName}
         ::= { saAmfCompCSTypeSupportedTable 1 }


     SaAmfCompCSTypeSupportedTableEntry ::= SEQUENCE {
         saAmfCompCSTypeSupportedCompName       SnmpAdminString,
         saAmfCompCSTypeSupportedCSIName        SnmpAdminString,
         saAmfCompCSTypeSupportedRowStatus      RowStatus
         }


     saAmfCompCSTypeSupportedCompName OBJECT-TYPE 
         SYNTAX      SnmpAdminString
         MAX-ACCESS  accessible-for-notify
         STATUS      current
         DESCRIPTION
             "An octet string that uniquely identifies a
              component name."
