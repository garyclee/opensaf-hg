         ::= { saAmfCSINameValueTableEntry 2 }

     saAmfCSINameValueParamValue OBJECT-TYPE 
         SYNTAX      SnmpAdminString
         MAX-ACCESS  read-create
         STATUS      current
         DESCRIPTION
             "The value string of the parameter. This is optional"
         ::= { saAmfCSINameValueTableEntry 3 }

     saAmfCSINameValueRowStatus OBJECT-TYPE 
         SYNTAX      RowStatus
         MAX-ACCESS  read-create
         STATUS      current
         DESCRIPTION
             "This variable is used to create or delete a row in
              this table. It can be set to delete or not-in-service
              only if saAmfCsiRowStatus is not in service or
              not ready."
         ::= { saAmfCSINameValueTableEntry 4 }

     --****************************************************************
     -- AMF  CS Type Name-paramName - This table designates what are the
     -- various params names that a CSI of a particular type supports.
     --****************************************************************
     saAmfCSTypeParamTable OBJECT-TYPE
         SYNTAX      SEQUENCE OF SaAmfCSTypeParamEntry
         MAX-ACCESS  not-accessible
         STATUS      current
         DESCRIPTION
             "This table designates what are
              the various param names that a
              CSI of a particular type supports"
         ::= { saAmfTables 13 } 

     saAmfCSTypeParamEntry OBJECT-TYPE 
         SYNTAX      SaAmfCSTypeParamEntry
         MAX-ACCESS  not-accessible
         STATUS      current
         DESCRIPTION
             "An entry in this table represents a parameter name
              associated with a CSI of a particular type."
         INDEX {saAmfCSTypeName, saAmfCSTypeParamName}
         ::= { saAmfCSTypeParamTable 1 }

     SaAmfCSTypeParamEntry ::= SEQUENCE {
         saAmfCSTypeName            SnmpAdminString, 
         saAmfCSTypeParamName       SnmpAdminString,
         saAmfCSTypeParamRowStatus  RowStatus
     }

    saAmfCSTypeName OBJECT-TYPE
         SYNTAX      SnmpAdminString
         MAX-ACCESS  accessible-for-notify
         STATUS      current
         DESCRIPTION
             "The name of the  CS Type in question whose parameters
              need to be viewed."
         ::= { saAmfCSTypeParamEntry 1 }

     saAmfCSTypeParamName OBJECT-TYPE
         SYNTAX      SnmpAdminString
         MAX-ACCESS  accessible-for-notify
         STATUS      current
         DESCRIPTION
             "The name of the parameter which belongs to the CS Type
              designated by saAmfCSTypeName."
         ::= { saAmfCSTypeParamEntry 2 }

     saAmfCSTypeParamRowStatus OBJECT-TYPE
         SYNTAX      RowStatus
         MAX-ACCESS  read-create
         STATUS      current
         DESCRIPTION
             "This value can be used to create or delete a row in
              this table. A row can only be added or deleted if there
              are no CSIs that are derived from the CSType of the row"
         ::= { saAmfCSTypeParamEntry 3 }


     --****************************************************************
     -- AMF Service unit Service Instance relationship   Table Objects
     --****************************************************************

          
     saAmfSUSITable OBJECT-TYPE
         SYNTAX      SEQUENCE OF SaAmfSUSITableEntry
         MAX-ACCESS  not-accessible
         STATUS      current
         DESCRIPTION
             "The Service unit - Service Instance relationship
              Table identifies all the SU SI relationships 
              that exists in the cluster. AMF adds entries
              to this table, there is no provision for an
              operator to create entries dynamically. This is
              because the AMF is responsible for determining
              SU assignments for SIs"
              
         ::= { saAmfTables 14 }

     saAmfSUSITableEntry OBJECT-TYPE
         SYNTAX      SaAmfSUSITableEntry
         MAX-ACCESS  not-accessible
         STATUS      current
         DESCRIPTION
             "An entry in this table represents a 
              SU SI relationship in the AMF
              cluster environment. An entry is
              automatically created and
              destroyed by the AMF system when
              a SI is assigned or un assigned
              to a SU."
         INDEX       {  saAmfSUSISuName, saAmfSUSISiName  }
         ::= { saAmfSUSITable 1 }

     SaAmfSUSITableEntry ::= SEQUENCE {
         saAmfSUSISuName                           SnmpAdminString,
         saAmfSUSISiName                           SnmpAdminString,
         saAmfSUSIHAState                          SaAmfHAState,
         saAmfSUSISGName                           SnmpAdminString 
     }

     saAmfSUSISuName OBJECT-TYPE 
         SYNTAX      SnmpAdminString
         MAX-ACCESS  accessible-for-notify
         STATUS      current
         DESCRIPTION
             "A octet string that
              uniquely identifies a
              service unit name that has
              been assigned an SI."
         ::= { saAmfSUSITableEntry 1 }

     saAmfSUSISiName OBJECT-TYPE 
         SYNTAX      SnmpAdminString
         MAX-ACCESS  accessible-for-notify
         STATUS      current
         DESCRIPTION
             "An octet string that
              uniquely identifies a
              service instance"
         ::= { saAmfSUSITableEntry 2 }


     saAmfSUSIHAState OBJECT-TYPE 
         SYNTAX      SaAmfHAState 
         MAX-ACCESS  read-only
         STATUS      current
         DESCRIPTION
             "This value denotes The current
              HA state for the SI assignment to an SU"
         ::= { saAmfSUSITableEntry 3 }

     saAmfSUSISGName OBJECT-TYPE 
         SYNTAX      SnmpAdminString 
         MAX-ACCESS  read-only
         STATUS      current
         DESCRIPTION
             "This value denotes the name of the SG 
              which contains the SU that is assigned the 
              particular SI designated in this table."
         ::= { saAmfSUSITableEntry 4 }



     --****************************************************************
     -- AMF Health Check  Table Objects
     --****************************************************************

          
     saAmfHealthCheckTable OBJECT-TYPE
         SYNTAX      SEQUENCE OF SaAmfHealthCheckTableEntry
         MAX-ACCESS  not-accessible
         STATUS      current
         DESCRIPTION
             "The Health Check Table identifies all
              the health check records that are used
              by components to perform health check.
              "
         ::= { saAmfTables 15 }

     saAmfHealthCheckTableEntry OBJECT-TYPE
         SYNTAX      SaAmfHealthCheckTableEntry
         MAX-ACCESS  not-accessible
         STATUS      current
         DESCRIPTION
             "An entry in this table represents a 
              record that is associated with a set
              of parameters (like periodicity and duration)."
         INDEX       {  saAmfHealthCompName, saAmfHealthCheckKey  }
         ::= { saAmfHealthCheckTable 1 }

     SaAmfHealthCheckTableEntry ::= SEQUENCE {
         saAmfHealthCompName                       SnmpAdminString,
         saAmfHealthCheckKey                       OCTET STRING,
         saAmfHealthCheckPeriod                    SafTimeStamp,
         saAmfHealthCheckMaxDuration               SafTimeStamp,
         saAmfHealthCheckRowStatus                 RowStatus
     }

     saAmfHealthCompName OBJECT-TYPE 
         SYNTAX      SnmpAdminString
         MAX-ACCESS  accessible-for-notify
         STATUS      current
         DESCRIPTION
             "An octet string that
              uniquely identifies a component"
         ::= { saAmfHealthCheckTableEntry 1 }

     saAmfHealthCheckKey OBJECT-TYPE 
         SYNTAX      OCTET STRING(SIZE(1..32))
         MAX-ACCESS  accessible-for-notify
         STATUS      current
         DESCRIPTION
             "An octet string that
              uniquely identifies a
              health check key name."
         ::= { saAmfHealthCheckTableEntry 2 }

     saAmfHealthCheckPeriod OBJECT-TYPE 
         SYNTAX      SafTimeStamp
         MAX-ACCESS  read-create
         STATUS      current
         DESCRIPTION
             "This is the time interval at
              which health check is performed
              by the component. The value is
              in units of nano seconds. The
              minimum value is
              10 milli seconds(10000000)
              writable only saAmfHealthCheckRowStatus
              is not active."
         ::= { saAmfHealthCheckTableEntry 3 }

     saAmfHealthCheckMaxDuration OBJECT-TYPE 
         SYNTAX      SafTimeStamp
         MAX-ACCESS  read-create
         STATUS      current
         DESCRIPTION
             "Time in milliseconds during which the AMF 
              expects the response to a healthcheck.
              The value is in units of
              nano seconds. The minimum
              value is 10 milli seconds(10000000)
              writable only saAmfHealthCheckRowStatus
              is not active."
         ::= { saAmfHealthCheckTableEntry 4 }

     saAmfHealthCheckRowStatus OBJECT-TYPE 
         SYNTAX      RowStatus
         MAX-ACCESS  read-create
         STATUS      current
         DESCRIPTION
             "This variable is used to create,
              modify, and/or delete a row in
              this table."
         ::= { saAmfHealthCheckTableEntry 5 }


     --****************************************************************
